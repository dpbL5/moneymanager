<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="gridPane" stylesheets="@../css/AddTransactionView.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="anchorPane" prefHeight="65.0" prefWidth="700.0" stylesheets="@../css/AddTransactionView.css">
         <children>
            <Label fx:id="dateLabel" layoutX="20.0" layoutY="23.0" prefHeight="20.0" prefWidth="90.0" text="date">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="categoryLabel" layoutX="200.0" layoutY="10.0" prefHeight="20.0" prefWidth="320.0" text="Category">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="noteLabel" layoutX="200.0" layoutY="33.0" prefWidth="320.0" text="note here">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Label>
            <Label fx:id="amountLabel" alignment="CENTER_RIGHT" layoutX="560.0" layoutY="10.0" prefHeight="20.0" prefWidth="120.0" text="10.000.000">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="currencyLabel" layoutX="680.0" layoutY="10.0" text="\$">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="fromAccount" alignment="CENTER_RIGHT" layoutX="560.0" layoutY="33.0" prefWidth="120.0" text="fromAccount">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
